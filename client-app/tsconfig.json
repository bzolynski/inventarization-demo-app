{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@src/*": ["./src/*"],
            "@assets/*": ["./src/assets/*"],
            "@components/*": ["./src/components/*"],
            "@screens/*": ["./src/screens/*"],
            "@redux-store/*": ["./src/redux-store/*"],
            "@theme/*": ["./src/theme/*"],
            "@models/*": ["./src/models/*"]
        }
    },
    "include": ["."]
}
